@model MHS_FINAL_PROJECT.Models.RegisterViewModel
@{
    ViewBag.Title = "Register";
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>login page</title>
    <link href="~/Content/accaount/StyleSheet1.css" rel="stylesheet" />
    <script src="https://code.iconify.design/1/1.0.6/iconify.min.js"></script>

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@100;200;300;400&display=swap" rel="stylesheet">

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Raleway:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800&display=swap" rel="stylesheet">

    <link href="~/Content/accaount/register.css" rel="stylesheet" />
</head>
<body>
    <div class="container_div">
        <div class="main">
            <div class="left_side">
                <div class="top_left">
                    <div class="content_top">
                        <a href="/Home">
                            <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="86" height="53" viewBox="0 0 86 53" fill="none">
                                <rect width="86" height="53" fill="url(#pattern0)" />
                                <defs>
                                    <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                                        <use xlink:href="#image0" transform="translate(0 -0.311321) scale(0.005 0.00811321)" />
                                    </pattern>
                                    <image id="image0" width="200" height="200" xlink:href="data:image/png;base64," />
                                </defs>
                            </svg>
                        </a>
                        <p>
                            Welcome to the website, pharmacist
                        </p>
                    </div>
                </div>

                <img class="doctors display_none" src="~/img/Register/regster_doctors.svg" />
            </div>
            <div class="right_side">
                <div class="top_right">
                    <p>Sign Up to mHS</p>
                    @Html.ValidationMessageFor(m => m.Privacy, "", new { @class = "" })
                </div>
                @using (Html.BeginForm("RegisterInfoPharmacist", "Account", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
                {
                    @Html.AntiForgeryToken()
                    if (ViewBag.UserName != "")
                    {
                        <span class="field-validation-error "style="width:100%; text-align:center; display: inline-block; ">@ViewBag.UserName</span>
                    }
                    if (ViewBag.email != "")
                    {
                        <span class="field-validation-error " style="width:100%; text-align:center; display: inline-block;">@ViewBag.email</span>
                    }
                    <div class="mid_right mid_right2">
                        <div class="tow_text_box">
                            <div class="input-box">
                                <div class="input-text">
                                    <span class="iconify" data-inline="false" data-icon="carbon:user-profile" style="font-size: 22px;"></span>
                                    @Html.TextBoxFor(m => m.Name, new { placeholder = "Name", @class = "", @autocomplete = "new-TextBox" })
                                </div>
                                @Html.ValidationMessageFor(m => m.Name, "", new { @class = "" })
                            </div>
                            <div class="input-box">
                                <div class="input-text">
                                    <img class="iconify" src="~/img/Register/username.svg" />
                                    @Html.TextBoxFor(m => m.Username, new { placeholder = "User Name", @class = "", @autocomplete = "new-TextBox" })
                                </div>
                                @Html.ValidationMessageFor(m => m.Username, "", new { @class = "" })
                            </div>
                        </div>


                        <div class="input-box">
                            <div class="input-text">
                                <img class="iconify" src="~/img/Register/email.svg" />
                                @Html.TextBoxFor(m => m.Email, new { placeholder = "Email", @class = "", @autocomplete = "new-TextBox" })
                            </div>
                            @Html.ValidationMessageFor(m => m.Email, "", new { @class = "" })
                        </div>


                        <div class="tow_text_box">
                            <div class="input-box">
                                <div class="input-text">
                                    <img class="iconify" src="~/img/Register/gender.svg" />
                                    @Html.DropDownListFor(m => m.gender, new SelectList(ViewBag.gender, "Text", "Value"), "gender", new { @class = "", @autocomplete = "new-DropDownList" })
                                </div>
                                @Html.ValidationMessageFor(m => m.gender, "", new { @class = "" })
                            </div>
                            <div class="input-box">
                                <div class="input-text">
                                    <img class="iconify" src="~/img/Register/age.svg" />
                                    @Html.TextBoxFor(m => m.Age, new { placeholder = "Age", @class = "", @autocomplete = "new-TextBox" })
                                </div>
                                @Html.ValidationMessageFor(m => m.Age, "", new { @class = "" })
                            </div>
                        </div>


                        <div class="input-box">
                            <div class="input-text">
                                <span class="iconify" data-inline="false" data-icon="teenyicons:password-outline" style="font-size: 22px;"></span>
                                @Html.PasswordFor(m => m.Password, new { placeholder = "Password", @class = "", autocomplete = "new-password" })
                            </div>
                            @Html.ValidationMessageFor(m => m.Password, "", new { @class = "" })
                        </div>


                        <div class="input-box">
                            <div class="input-text">
                                <span class="iconify" data-inline="false" data-icon="teenyicons:password-outline" style="font-size: 22px;"></span>
                                @Html.PasswordFor(m => m.ConfirmPassword, new { placeholder = "Confirm Password", @class = "", autocomplete = "new-password" })
                            </div>
                            @Html.ValidationMessageFor(m => m.ConfirmPassword, "", new { @class = "" })
                        </div>
                        <div class="input-text">
                            <div class="Privacy">
                                @Html.CheckBoxFor(m => m.Privacy, new { @class = "box " })
                                <p>Creating an account means you’re okay with our <a href="#">Terms of Service</a> and <a href="#">Privacy Policy</a>,</p>
                            </div>
                        </div>


                    </div>
                    <div class="buttom_right">
                        <input type="submit" value="CONTINUE" class="btn-login" />
                        <p class="create-account">Already a member? <a class="h_ref" href="~/Account/Login">Sign In</a></p>
                    </div>
                }
            </div>

            <div class="top_circle"></div>
            <div class="Bottom_triangle"></div>
        </div>
    </div>
</body>
</html>




@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
